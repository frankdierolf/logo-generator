{
  "name": "@logocli/logo-generator",
  "version": "1.0.0",
  "license": "MIT",
  "exports": "./main.ts",
  "imports": {
    "@cliffy/command": "jsr:@cliffy/command@1.0.0-rc.7",
    "@cliffy/prompt": "jsr:@cliffy/prompt@1.0.0-rc.7",
    "@cliffy/ansi": "jsr:@cliffy/ansi@1.0.0-rc.7",
    "@std/fs": "jsr:@std/fs@^1.0.0",
    "@std/path": "jsr:@std/path@^1.0.0",
    "@std/crypto": "jsr:@std/crypto@^1.0.0",
    "@std/encoding": "jsr:@std/encoding@^1.0.0",
    "openai": "jsr:@openai/openai@^5.12.2",
    "p-limit": "npm:p-limit@^6.0.0"
  },
  "tasks": {
    "dev": "deno run --allow-all --watch main.ts",
    "build": "deno compile --allow-all --output bin/logo-cli main.ts",
    "install": "deno install --allow-all --name logo-cli main.ts",
    "test": "deno test --allow-all"
  },
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true
  }
}
